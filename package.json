{
  "name": "rw-api-microservice-node",
  "version": "5.0.0-beta.2",
  "description": "Integration library for the RW API microservice",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "coverage": "nyc ts-mocha -b --project tsconfig.json -r tsconfig-paths/register --timeout 20000 'test/**/*.ts' --exit",
    "test": "ts-mocha -b --project tsconfig.json -r tsconfig-paths/register --timeout 20000 'test/**/*.ts' --exit",
    "build": "tsc --project tsconfig.json"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "*.test.*"
    ],
    "include": [
      "src/**"
    ],
    "reporter": [
      "text-summary",
      "lcov"
    ],
    "reportDir": "coverage",
    "all": true
  },
  "keywords": [
    "koa1",
    "koa2",
    "rw",
    "nodejs"
  ],
  "author": {
    "name": "Vizzuality",
    "email": "hello@vizzuality.com",
    "url": "https://vizzuality.com/"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist/**/*"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/resource-watch/rw-api-microservice-node/issues"
  },
  "homepage": "https://github.com/resource-watch/rw-api-microservice-node",
  "dependencies": {
    "@koa/cors": "^3.4.3",
    "@tiagojsag/fastly-promises": "^1.0.1",
    "aws-sdk": "^2.1405.0",
    "axios": "^1.4.0",
    "koa-compose": "^4.1.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.8",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/debug": "^4.1.8",
    "@types/koa": "2.13.6",
    "@types/koa-convert": "^1.2.4",
    "@types/koa-qs": "^2.0.0",
    "@types/koa-router": "7.4.4",
    "@types/koa__cors": "3.3.1",
    "@types/mocha": "^9.1.1",
    "@types/request-promise": "^4.1.48",
    "bunyan": "^1.8.15",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.4.0",
    "koa-body": "^6.0.1",
    "koa-convert": "^2.0.0",
    "koa-qs": "^3.0.0",
    "koa-router": "^12.0.0",
    "koa1": "npm:koa@^1.7.0",
    "koa": "npm:koa@^2.14.2",
    "mocha": "^10.2.0",
    "nock": "^13.3.1",
    "nyc": "^15.1.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5"
  }
}
