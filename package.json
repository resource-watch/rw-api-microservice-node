{
  "name": "rw-api-microservice-node",
  "version": "1.0.0",
  "description": "Integration library for the RW API microservice",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc ts-mocha -b --project tsconfig.json -r tsconfig-paths/register --timeout 20000 'test/**/*.ts' --exit",
    "test": "ts-mocha -b --project tsconfig.json -r tsconfig-paths/register --timeout 20000 'test/**/*.ts' --exit"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "*.test.*"
    ],
    "include": [
      "src/**"
    ],
    "reporter": [
      "text-summary",
      "lcov"
    ],
    "reportDir": "coverage",
    "all": true
  },
  "keywords": [
    "dataset",
    "koa2",
    "rw",
    "nodejs"
  ],
  "author": {
    "name": "Vizzuality",
    "email": "hello@vizzuality.com",
    "url": "http://vizzuality.com/"
  },
  "engines": {
    "node": ">=8"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/resourcewatch/rw-api-microservice-node/issues"
  },
  "homepage": "https://github.com/resourcewatch/rw-api-microservice-node",
  "dependencies": {
    "bluebird": "^3.7.2",
    "debug": "^4.3.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.2.14",
    "@types/debug": "^4.1.5",
    "@types/koa": "^2.11.6",
    "@types/mocha": "^8.0.4",
    "@types/request-promise": "^4.1.46",
    "bunyan": "^1.8.14",
    "chai": "^4.2.0",
    "koa1": "npm:koa@^1.7.0",
    "koa2": "npm:koa@^2.13.0",
    "mocha": "^8.2.1",
    "nock": "^13.0.4",
    "nyc": "^15.1.0",
    "ts-mocha": "^8.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  }
}
