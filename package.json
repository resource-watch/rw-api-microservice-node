{
  "name": "rw-api-microservice-node",
  "version": "3.4.0",
  "description": "Integration library for the RW API microservice",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "coverage": "nyc ts-mocha -b --project tsconfig.json -r tsconfig-paths/register --timeout 20000 'test/**/*.ts' --exit",
    "test": "ts-mocha -b --project tsconfig.json -r tsconfig-paths/register --timeout 20000 'test/**/*.ts' --exit",
    "build": "tsc --project tsconfig.json"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "*.test.*"
    ],
    "include": [
      "src/**"
    ],
    "reporter": [
      "text-summary",
      "lcov"
    ],
    "reportDir": "coverage",
    "all": true
  },
  "keywords": [
    "koa1",
    "koa2",
    "rw",
    "nodejs"
  ],
  "author": {
    "name": "Vizzuality",
    "email": "hello@vizzuality.com",
    "url": "http://vizzuality.com/"
  },
  "engines": {
    "node": ">=11"
  },
  "files": [
    "dist/**/*"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/resource-watch/rw-api-microservice-node/issues"
  },
  "homepage": "https://github.com/resource-watch/rw-api-microservice-node",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "axios": "^0.21.1",
    "fastly-promises": "tiagojsag/fastly-promises#master",
    "koa-compose": "^4.1.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/debug": "^4.1.5",
    "@types/koa": "^2.11.6",
    "@types/koa-convert": "^1.2.3",
    "@types/koa-qs": "^2.0.0",
    "@types/koa-router": "^7.4.1",
    "@types/koa__cors": "^3.0.2",
    "@types/mocha": "^8.0.4",
    "@types/request-promise": "^4.1.46",
    "bunyan": "^1.8.14",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "koa-body": "^4.2.0",
    "koa-convert": "^2.0.0",
    "koa-qs": "^3.0.0",
    "koa-router": "^10.0.0",
    "koa1": "npm:koa@^1.7.0",
    "koa2": "npm:koa@^2.13.0",
    "mocha": "^7.2.0",
    "nock": "^13.0.4",
    "nyc": "^15.1.0",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.5"
  }
}
